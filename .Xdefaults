*font: -*-fixed-medium-r-*-*-12-*-*-*-*-*-*-*
*boldFont: -*-fixed-medium-r-*-*-12-*-*-*-*-*-*-*


URxvt*scrollBar: false
URxvt*internalBorder: 4

URxvt*perl-lib: /usr/lib/urxvt/perl
URxvt*perl-ext-common: default,option-popup,matcher
URxvt*urlLauncher: /usr/bin/firefox
URxvt*matcher.button: 1

URxvt*termName: xterm

URxvt*foreground:       #FFFFFF
URxvt*background:       #191919
URxvt*colorBD:          #EEEEEF

! Black
URxvt*color0:           #404040
!URxvt*color8:           #252525

! Red
URxvt*color1:           #C12121
URxvt*color9:           #E50E0E

! Green
URxvt*color2:           #75BD00
URxvt*color10:          #89b83f

! Yellow
URxvt*color3:           #EDD76A
URxvt*color11:          #efef60

! Blue
URxvt*color4:           #4F6FAE
URxvt*color12:          #3F6FD0

! Magenta
URxvt*color5:           #706c9a
URxvt*color13:          #826ab1

! Cyan
URxvt*color6:           #508695
URxvt*color14:          #a1cdcd

! White
URxvt*color7:           #BBBBBB
URxvt*color15:          #EEEEEF

Xcursor*theme: neutral
